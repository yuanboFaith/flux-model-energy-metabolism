theme.myClassic +
scale_y_continuous(expand = expansion(mult = c(0, 0)),
breaks = seq(0, 100, 20)) +
scale_x_discrete(expand = expansion(add = .8)) +
labs(x = NULL, y = "amino acid %") +
scale_fill_manual(values = c("grey90", "orange", "steelblue1", "tomato"))
}
